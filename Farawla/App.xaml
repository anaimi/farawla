<Application x:Class="Farawla.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="clr-namespace:Farawla.Core"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Resources/Theme/ScrollBar.xaml" />
				<ResourceDictionary Source="Resources/Theme/CheckBox.xaml" />
				<ResourceDictionary Source="Resources/Theme/ComboBox.xaml" />
				<ResourceDictionary Source="Resources/Theme/CompletionWindow.xaml" />
				<ResourceDictionary Source="Resources/Theme/MainTabControl.xaml" />
				<ResourceDictionary Source="Resources/Theme/Misc.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
